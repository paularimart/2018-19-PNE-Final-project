http://localhost:8000/

<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>MAIN</title>
  </head>
  <body style="background-color: #DAF7A6;">
    <h3>CHOOSE AN OPTION </h3>

    <h3>List of species </h3>
    <form action="listSpecies" method="get">
    Print all the species on the list
    <br><br>
        <input type="submit"/>
    </form>

    <h3>List of species </h3>
    <form action="listSpecies" method="get">
      Limit:  <input type="text" name="limit">
      <br><br>
        <input type="submit"/>
    </form>

    <h3>Karyotype </h3>
    <form action="karyotype" method="get">
      Specie:  <input type="text" name="specie">
      <br><br>
        <input type="submit"/>
    </form>

    <h3>Chromosome length </h3>
    <form action="chromosomeLength" method="get">
      Specie:  <input type="text" name="specie">
      <br><br>
      Chromosome:  <input type="text" name="chromo">
      <br><br>
        <input type="submit"/>
    </form>

    <h3>Human gene sequence </h3>
    <form action="geneSeq" method="get">
      Gene:  <input type="text" name="gene">
      <br><br>
        <input type="submit"/>
    </form>

    <h3>Human gene information </h3>
    <form action="geneInfo" method="get">
      Gene:  <input type="text" name="gene">
      <br><br>
        <input type="submit"/>
    </form>

    <h3>Human gene calculations </h3>
    <form action="geneCalc" method="get">
      Gene:  <input type="text" name="gene">
      <br><br>
        <input type="submit"/>
    </form>

    <h3>Gene List </h3>
    <form action="geneList" method="get">
      Chromosome:  <input type="text" name="chromo">
      <br><br>
      Start:  <input type="text" name="start">
      <br><br>
      End:  <input type="text" name="end">
      <br><br>
        <input type="submit"/>
    </form>

  </body>
</html>

